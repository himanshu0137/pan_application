extends layout

block content
  .page-header
    h3 Users

  table
    tr
      th S. No.
      th Agent Name/ Email
      th Current Balance
      th Edit
      th
    each user, index in users
      tr
        td= index+1
        td= user.name || user.email
        td= "Rs. " + user.balance
        td
          a(href="./edituser/"+user.id)| Edit
